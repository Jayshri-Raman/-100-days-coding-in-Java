import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String s=sc.nextLine();
      int m=s.charAt(0)-'0';
      int n=s.charAt(1)-'0';
     for(int i=0;i<m;i++)
     {
         System.out.print("1");
     }
     for(int i=0;i<n;i++)
     {
         System.out.print("0");
     }
        
    }
}
