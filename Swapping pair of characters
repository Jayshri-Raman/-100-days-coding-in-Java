

import java.util.Arrays;

class HelloWorld {
    public static void main(String[] args) {
        String s="jaiajsjdk";
        char arr[]=s.toCharArray(); //convert string to char array
       char t;
       for(int i=0;i<arr.length-1;i=i+2)//pair of char
       {
           t=arr[i];
           arr[i]=arr[i+1];
           arr[i+1]=t;
       }
       System.out.print(arr);
    }
}


